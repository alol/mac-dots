[user]
  name = Ed James
  email = ed.james.email@gmail.com
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  editor = subl -n -w
[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[branch]
  autosetupmerge = true
[remote "origin"]
  push = HEAD
[push]
  default = tracking
[diff]
  mnemonicprefix = true
  tool = diffmerge
[difftool]
  prompt = false
[difftool "diffmerge"]
  cmd = diffmerge $LOCAL $REMOTE
[mergetool]
  keepBackup = false
